@model List<FastFoodWebsite.Models.ORDERDETAIL>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutPageAdmin.cshtml";
}

<div class="details">
    <div class="recentOrders">
        <div class="cardHeader">
            <h3>Recent Order</h3>
        </div>
        <table>
            <thead>
                <tr>
                    <td>ORDER_ID</td>
                    <td>PRODUCT_ID</td>
                    <td>PRODUCT_NAME</td>
                    <td>NOTES</td>
                    <td>QUANTITY</td>
                    <td>PRICE</td>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.ORDERID</td>
                        <td>@item.PRODUCTID</td>
                        <td>@item.PRODUCT.PRODUCTNAME</td>
                        <td>@item.NOTES</td>
                        <td>@item.QUANTITY</td>
                        <td>@item.PRICE</td>
                    </tr>
                }

            </tbody>
        </table>
    </div>
    <div>


    </div>
</div>

